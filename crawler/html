<script type="text/javascript">
	$(function() {
		$('.lbul1 li a').click(function() {
			// $('.lbul1 li a').removeClass('lbchange')
			$(this).addClass('lbchange').siblings('a').removeClass('lbchange');
			generalUrl();
		});
		$("#downup span").click(
				function() {
					$(this).addClass("duchange").siblings("span").removeClass(
							"duchange");
					generalUrl();
				});
		$("#query").click(function(){
			
			//window.location.href = "/bonds_list.html" + "?inpquery="+$("#inpquery").val();
			var uri = "/bonds_list.html";			
			var param = {};
			param.inpquery = $("#inpquery").val();
			
			var gohtml ="<form id='form_go_html' name='form_go_html' action='"+uri+"' method='post' >";
	     for(key in param) {
	     gohtml += "<input type='hidden' name='"+key+"' value='"+param[key]+"' />";
	     }
	     // gohtml += "<input type='submit' name='submit_go_html' />"
	     gohtml += "</form>"
	      $("body").append(gohtml);
	     // document.getElementById("form_go_html").submit()
	     $("#form_go_html").submit();

			
			//交易  
			
		});
		var deadline = "";

		if (deadline) {
			$("li[type=deadline] a.lbchange").removeClass("lbchange");
			$("li[type=deadline] a[maxdeadline='" + deadline + "']").addClass(
					"lbchange");
		}
		var annualrate = "";
		if (annualrate) {
			$("li[type=annualrate] a.lbchange").removeClass("lbchange");
			$("li[type=annualrate] a[maxannualrate='" + annualrate + "']")
					.addClass("lbchange");
		}
		var mintenderedsum = "";
		if (mintenderedsum) {
			$("li[type=mintenderedsum] a.lbchange").removeClass("lbchange");
			$(
					"li[type=mintenderedsum] a[minmintenderedsum='"
							+ mintenderedsum + "']").addClass("lbchange");
		}
		$("#downup span").bind("click", function() {
			$("#downup span").removeClass("duchange");
			if ($(this).addClass("duchange").attr("sortinfo") == "1") {
				$("span", $(this).attr("sortinfo", "0")).html("&darr;");
			} else {
				$("span", $(this).attr("sortinfo", "1")).html("&uarr;");
			}
			generalUrl($pager.currentPage);
		});
		var sort = '99 1';
		if (sort) {
			$("#downup span").removeClass("duchange");
			var temp = sort.split(" ");
			var $t = $("span[sortid='" + temp[0] + "']").addClass("duchange");
			if (temp[1] == "1") {
				$("span", $t.attr("sortinfo", temp[1])).html("&uarr;");
			} else {
				$("span", $t.attr("sortinfo", temp[1])).html("&darr;");
			}
		}
		var urlparam = generalParam();
		$pager = $.kd.kdPager.init({
			currentPage : '1',
			totalPage : '63',
			totalRecords : parseInt('622'),
			prefix : '/bonds_list.html',
			suffix : function(n) {
				return urlparam + '&page=' + n+'&inpquery_en=';
			}
		});
	});
	function generalParam() {
		var maxdeadline = $("a.lbchange", "li[type=deadline]").attr(
				"maxdeadline");
		var mindeadline = $("a.lbchange", "li[type=deadline]").attr(
				"mindeadline");
		var maxannualrate = $("a.lbchange", "li[type=annualrate]").attr(
				"maxannualrate");
		var minannualrate = $("a.lbchange", "li[type=annualrate]").attr(
				"minannualrate");
		var maxmintenderedsum = $("a.lbchange", "li[type=mintenderedsum]")
				.attr("maxmintenderedsum");
		var minmintenderedsum = $("a.lbchange", "li[type=mintenderedsum]")
				.attr("minmintenderedsum");
		var reg = /[\w\W]+/, param = "";
		if (maxdeadline && reg.test(maxdeadline))
			param += (reg.test(param) ? "&" : "")
					+ ("maxdeadline=" + maxdeadline);
		if (mindeadline && reg.test(mindeadline))
			param += (reg.test(param) ? "&" : "")
					+ ("mindeadline=" + mindeadline);
		if (maxannualrate && reg.test(maxannualrate))
			param += (reg.test(param) ? "&" : "")
					+ ("maxannualrate=" + maxannualrate);
		if (minannualrate && reg.test(minannualrate))
			param += (reg.test(param) ? "&" : "")
					+ ("minannualrate=" + minannualrate);
		if (maxmintenderedsum && reg.test(maxmintenderedsum))
			param += (reg.test(param) ? "&" : "")
					+ ("maxmintenderedsum=" + maxmintenderedsum);
		if (minmintenderedsum && reg.test(minmintenderedsum))
			param += (reg.test(param) ? "&" : "")
					+ ("minmintenderedsum=" + minmintenderedsum);
		var $order = $("#downup .duchange"), sort = "sortid="
				+ $order.attr("sortid") + "&" + "sortinfo="
				+ $order.attr("sortinfo");
		param += reg.test(param) ? ("&" + sort) : ("" + sort);
		return param;
	}
	function generalUrl(page) {
		var param = generalParam();
		if (page) {
			param += (param ? "&page=" : "?page=") + page;
		}
		window.location.href = "/bonds_list.html" + (param ? "?" + param : "");
	}
</script>
