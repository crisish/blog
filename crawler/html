<script type="text/javascript">
	//window.umesession = document.cookie.replace("undefined; ", "").replace(" undefined;", "");
	//上传图片会导致cookie中sessionid变化，故取出jsessionid保存起来，上传成功后写回
	//若cookie中有多个值 时，直接document.cookie = window.umesession，cookie值 不会变化
			var cookies =  document.cookie.split(";");
			for (idx in cookies) {
			if ( cookies[idx].indexOf("JSESSIONID") > -1) {
				window.umesession = cookies[idx];				 
			   }
			}
			
	var fileaction = "https://www.zhelitou.com.cn/fileserver/file!upload.do";
    var fileserver = "https://www.zhelitou.com.cn/fileserver";
</script>
